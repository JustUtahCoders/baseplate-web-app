{
  "name": "@baseplate-sdk/web-app",
  "version": "2.0.0",
  "description": "Customer web app for Baseplate",
  "type": "module",
  "engines": {
    "node": ">=17.5"
  },
  "types": "types/backend/TSEntry.d.ts",
  "main": "this-project-only-exposes-typescript-declarations.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "types"
  ],
  "scripts": {
    "develop": "concurrently \"pnpm:develop:*\"",
    "dev:vscode": "concurrently \"pnpm:develop:db\" \"pnpm:develop:frontend\" \"pnpm:develop:backend:types\"",
    "develop:backend": "cross-env \"NODE_OPTIONS=--experimental-loader @node-loader/core  --experimental-specifier-resolution=node --experimental-fetch\" IS_RUNNING_LOCALLY=true BASEPLATE_ENV=dev nodemon --signal SIGINT backend/Server.ts --watch backend --watch migrations --watch frontend --ext tsx,ts,js",
    "develop:backend:types": "tsc --noEmit --watch --preserveWatchOutput",
    "develop:db": "docker-compose up",
    "develop:frontend": "webpack serve --config webpack.config.cjs --port 7700",
    "check-format": "prettier --check .",
    "clean": "rimraf dist lib",
    "build": "pnpm run clean && concurrently pnpm:build:*",
    "build:backend": "tsc",
    "build:frontend": "webpack build --mode=production --env prod",
    "start": "node --experimental-specifier-resolution=node backend/Server.js",
    "format": "prettier --write .",
    "lint": "eslint \"backend/**/*\" \"frontend/**/*\"",
    "test": "concurrently \"pnpm:test:*\"",
    "test:frontend": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.browser.config.js --coverage",
    "test:db": "pnpm run test-db-migrations && pnpm run test-db-code",
    "prepare-test-db": "bash backend/DB/TestHelpers/StartTestDB.sh && cross-env NODE_ENV=db-tests pnpm exec sequelize db:migrate && cross-env NODE_ENV=db-tests pnpm exec sequelize db:seed:all",
    "destroy-test-db": "docker-compose -f backend/DB/TestHelpers/docker-compose.tests.yml down -v",
    "test-db-migrations": "bash backend/DB/TestHelpers/MigrationTests.sh",
    "test-db-code": "cross-env \"NODE_OPTIONS=--experimental-vm-modules --experimental-fetch\" NODE_ENV=db-tests jest --config jest.db.config.js --detectOpenHandles",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustUtahCoders/baseplate-web-app.git"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 2 Safari versions",
    "last 3 Edge versions",
    "last 10 Firefox versions"
  ],
  "author": "Joel Denning",
  "license": "See LICENSE file",
  "bugs": {
    "url": "https://github.com/JustUtahCoders/baseplate-web-app/issues"
  },
  "homepage": "https://github.com/JustUtahCoders/baseplate-web-app#readme",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-import-assertions": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@changesets/changelog-github": "^0.4.3",
    "@changesets/cli": "^2.21.1",
    "@jest/globals": "^28.0.0",
    "@jest/types": "^28.0.0",
    "@node-loader/core": "^2.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@vercel/ncc": "^0.33.3",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "husky": "^7.0.4",
    "jest": "^28.0.0",
    "jest-cli": "^28.0.0",
    "jest-environment-jsdom": "^28.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.0",
